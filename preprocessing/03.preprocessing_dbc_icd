","# perform imputation on missing columns as recommend by stef van buuren\n","\n","imp_mean = IterativeImputer(random_state=0)\n","# create a spec_bin, related to whether the line is orthopaedic patient\n","ort_cond = df_impute['Opnamespecialisme'] == 'ORT'\n","df_impute['Spec_bin'] = np.where(ort_cond, 1, 0)\n","df_impute['BMI_score'] = df_impute['BMI_score'].replace(',', '.').astype(float)\n","X = df_impute[['Opnameleeftijd', 'Geslacht',\n","       'ASA_score', 'Dyspnoe_score', 'BMI_score', 'Spec_bin', 'SES']]\n","print(f\"mean value of x[bmi] before = {X['BMI_score'].mean()}\")\n","imp_mean.fit(X)\n","imputed = imp_mean.transform(df_impute[['Opnameleeftijd', 'Geslacht',\n","       'ASA_score', 'Dyspnoe_score', 'BMI_score', 'Spec_bin', 'SES',]])\n","# convert imputed back to df\n","df_impute[['Opnameleeftijd', 'Geslacht',\n","       'ASA_score', 'Dyspnoe_score', 'BMI_score', 'Spec_bin', 'SES']] = imputed\n","print(f\"mean value of x[bmi] = {df_impute['BMI_score'].mean()}\")\n","df_impute.loc[df_impute['Dyspnoe_score'] < 0, 'Dyspnoe_score'] = 0\n","\n","df_impute['ICD_CODE'] = df['ICD_CODE']\n","print(df_impute.isna().sum()[:])\n","df = df_impute.copy()"],"metadata":{"id":"mF2F2xyqIxtU","executionInfo":{"status":"ok","timestamp":1734539396604,"user_tz":-60,"elapsed":3747,"user":{"displayName":"Nathan de Pater","userId":"00637828017025432291"}},"colab":{"base_uri":"https://localhost:8080/"},"outputId":"97dab474-123c-443f-e64b-148cf1a6327e"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["mean value of x[bmi] before = 27.115940476190477\n","mean value of x[bmi] = 27.130332145544095\n","Patiëntnummer        0\n","Opnameleeftijd       0\n","Opnamenummer         0\n","OpnameDatumTijd      0\n","Geslacht             0\n","                    ..\n","DBC_327              0\n","DBC_333              0\n","DBC_218              0\n","Ontslagbestemming    0\n","Spec_bin             0\n","Length: 68, dtype: int64\n"]}]},{"cell_type":"markdown"